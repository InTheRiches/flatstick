{
  "expo": {
    "name": "Flatstick",
    "slug": "putt-perfect",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/branding/appicon/play_store_512.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#ffffff",
    "newArchEnabled": true,
    "ios": {
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.intheriches.flatstick",
      "supportsTablet": false,
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "androidNavigationBar": {
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/branding/appicon/adaptive_icon.png",
        "monochromeImage": "./assets/branding/appicon/ic_launcher_monochrome.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.intheriches.flatstick",
      "googleServicesFile": "./google-services.json",
      "versionCode": 4
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["assets/fonts/Geist[wght].ttf"]
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-2701716227191721~2104236424",
          "iosAppId": "ca-app-pub-2701716227191721~9292578841"
        }
      ],
      "expo-secure-store",
      ["react-native-bootsplash", { "assetsDir": "assets/bootsplash" }],
      ["./android-manifest-launch-mode-plugin"],
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5be751a1-abea-409b-adde-2a402e979c5c"
      }
    },
    "owner": "puttlab"
  }
}
