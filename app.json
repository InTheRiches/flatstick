{
  "expo": {
    "name": "Flatstick",
    "slug": "putt-perfect",
    "version": "1.1.4",
    "orientation": "portrait",
    "icon": "./assets/branding/appicon/play_store_512.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#ffffff",
    "newArchEnabled": true,
    "ios": {
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.intheriches.flatstick",
      "supportsTablet": false,
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow $(PRODUCT_NAME) to access your location when the app is in use.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow $(PRODUCT_NAME) to access your location even when the app is in the background.",
        "NSLocationAlwaysUsageDescription": "Allow $(PRODUCT_NAME) to access your location even when the app is in the background."
      }
    },
    "androidNavigationBar": {
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/branding/appicon/adaptive_icon.png",
        "monochromeImage": "./assets/branding/appicon/ic_launcher_monochrome.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.intheriches.flatstick",
      "googleServicesFile": "./google-services.json",
      "versionCode": 9,
      "permissions": [
        "NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-notifications",
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "assets/fonts/Geist[wght].ttf"
          ]
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-2701716227191721~2104236424",
          "iosAppId": "ca-app-pub-2701716227191721~9292578841"
        }
      ],
      "expo-secure-store",
      [
        "react-native-bootsplash",
        {
          "assetsDir": "assets/bootsplash"
        }
      ],
      [
        "./android-manifest-launch-mode-plugin"
      ],
      "@react-native-google-signin/google-signin",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Flatstick to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5be751a1-abea-409b-adde-2a402e979c5c"
      }
    },
    "owner": "puttlab"
  }
}
